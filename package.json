{
  "name": "nodegit-flow",
  "version": "0.1.4",
  "description": "nodegit-flow contains gitflow methods that aren't include in the vanilla nodegit package",
  "main": "build/src/index.js",
  "scripts": {
    "compile": "babel --presets es2015 -d ./build/spec ./spec && babel --presets es2015 -d ./build/src ./src",
    "eslint": "./node_modules/.bin/eslint src",
    "prepublish": "babel --presets es2015 -d ./build/src ./src",
    "test": "npm run eslint && npm run compile && ./node_modules/.bin/jasmine-node build/spec"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/smith-kyle/nodegit-flow.git"
  },
  "keywords": [
    "node",
    "git",
    "gitflow",
    "nodegit"
  ],
  "files": [
    "build/src"
  ],
  "author": "Jose Garcia",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/smith-kyle/nodegit-flow/issues"
  },
  "homepage": "https://github.com/smith-kyle/nodegit-flow#readme",
  "dependencies": {
    "nodegit": "^0.5.0"
  },
  "devDependencies": {
    "babel-cli": "^6.3.15",
    "babel-preset-es2015": "^6.3.13",
    "eslint": "^1.9.0",
    "jasmine-node": "mhevery/jasmine-node#Jasmine2.0",
    "jasmine-pit": "^2.0.2"
  }
}
